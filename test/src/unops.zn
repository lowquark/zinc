
module my_module {
  function main() -> (int64 ret) {
    int64 a = 10;

    if(!10 != 0) { return 1; }
    if(! a != 0) { return 2; }

    if(~~10 != 10) { return 3; }
    if(~~ a != 10) { return 4; }

    if((-10)*(-10) !=  100) { return 3; }
    if(( 10)*(-10) != -100) { return 4; }
    if((-a)*(-a) !=  100)   { return 5; }
    if(( a)*(-a) != -100)   { return 6; }

    return 0;
  }
}

